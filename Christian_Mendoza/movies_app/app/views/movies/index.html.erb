<h1>Welcome to RetFlex!</h1>
<h4>See our collection of movies below:</h4>

<table>
  <thead>
    <tr>
      <th>Year Released</th>
      <th>Title</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <% @movies.each do |movie| %>
    <tr>
      <td><%= movie.year_released %></td>
      <td><%= movie.title %></td>
      <td><%= movie.description %></td>
    </tr>
    <% end %>
  </tbody>
</table>